# Quality Gates Environment Configuration
# This file contains environment variables specifically for running quality gate tests

# Core Environment Settings
BASIS_ENVIRONMENT=dev
BASIS_EXECUTION_MODE=backtest
BASIS_DEPLOYMENT_MODE=local

# Data and Results Directories
BASIS_DATA_DIR=data
BASIS_RESULTS_DIR=results

# Logging Configuration
BASIS_DEBUG=false
BASIS_LOG_LEVEL=INFO

# Data Date Range for Quality Gates
BASIS_DATA_START_DATE=2024-05-01
BASIS_DATA_END_DATE=2024-06-02

# Quality Gate Specific Settings
BASIS_QUALITY_GATE_MODE=true
BASIS_SKIP_LIVE_TESTS=true
BASIS_FAST_MODE=true
